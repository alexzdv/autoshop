%h2 Sign up

=form_for(@admin, :url => admin_panel_admins_path) do |f|

  %p
    =f.label :email
    %br
    =f.text_field :email

  %p
    =f.label :password
    %br
    =f.password_field :password

  %p
    =f.label :password_confirmation
    %br
    =f.password_field :password_confirmation

  %p= f.submit "Sign up"
